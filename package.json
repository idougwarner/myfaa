  "scripts": {
    "clean": "rm -rf $OUTPUT_DIR",
    "build:client": "node webpack/build.js",
    "build:server": "babel ./server -d ./build/server --source-maps --copy-files",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "knex:migrate": "nf run knex migrate:latest",
    "knex:make-migration": "nf run knex migrate:make",
    "start:client": "better-npm-run start:client",
    "start:server": "better-npm-run start:server",
    "start:dev": "run-p start:server start:client",
    "lint": "eslint  --ext .js,.vue client server",
    "test": "better-npm-run test",
    "test:e2e": "better-npm-run test:e2e"
  },
  "betterScripts": {
    "start:client": "webpack-dev-server --inline --progress --config webpack/webpack.dev.conf.js",
    "start:server": "nodemon --watch ./server --exec babel-node server",
    "test": "jest --runInBand --forceExit",
    "test:e2e": "jest --runInBand --config jest.config.e2e.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
